	<bean id='message' class="ex3_autowired.MessageBeanImpl">
		<property name="name" value="지각자"/> <!--setName() 호출  -->
		<property name="message">
		<value>조원들게 쏘세요</value>
		</property><!-- setMessage 호출 -->
	</bean>
